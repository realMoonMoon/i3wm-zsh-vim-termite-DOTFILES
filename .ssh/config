Host db
    HostName example.com
    Port 22
    User example
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 600
    AddKeysToAgent yes
    IdentityFile ~/.ssh/bastion-vilja
